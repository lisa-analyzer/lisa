class A {	
	a;
	b;

	~A(x, y) {
		this.a = x;
		this.b = y;
	}
	
	f1() {
		def o = new A(1,2);
		o.a = 1;	
		o.b = 2;
	}

	f2() {
		def o = new A(1,2);
		o.a = 1;	
		o.b = o.a + 1;	
		
		for (def i = 0; i < 10; i = i + 1) {
			o.a = o.a + 1;
		}
	}
	
	f3() {
		def o1 = new A(1,2);
		o1.a = 1;	
		o1.b = 1;	
	}
	
	f4() {
		def o = new A(1,2);
		def i = 0;
		while(true) {
			o.a = 0;
			i = i + 1;
		}
	}
	
	f5() {
		def x = new A(1,2);
		def y = x;
		x.a = 1;
		def a = x.a + y.a;
	}
}
